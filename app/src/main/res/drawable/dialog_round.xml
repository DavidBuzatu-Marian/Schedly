<?xml version="1.0" encoding="UTF-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="#FFFFFF" />
            <corners android:radius="30dp" />
            <padding
                android:bottom="10dp"
                android:left="10dp"
                android:right="10dp"
                android:top="10dp" />
        </shape>
    </item>
    <item
        android:bottom="-60dp"
        android:gravity="bottom|left"
        android:left="-50dp">
        <shape android:shape="oval">
            <gradient
                android:angle="90"
                android:endColor="#216D6A"
                android:startColor="#174240"
                android:type="linear" />
            <corners android:radius="0dp" />
            <size
                android:width="150dp"
                android:height="150dp" />
        </shape>
    </item>
    <item
        android:bottom="-70dp"
        android:gravity="bottom|left"
        android:left="50dp">
        <shape android:shape="oval">
            <gradient
                android:angle="90"
                android:endColor="#174240"
                android:startColor="#2B8C87"
                android:type="linear" />
            <corners android:radius="0dp" />
            <size
                android:width="120dp"
                android:height="120dp" />
        </shape>
    </item>
</layer-list>

