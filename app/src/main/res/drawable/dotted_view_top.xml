<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center" >
        <shape android:shape="rectangle">
            <size
                android:height="48dp"
                android:width="48dp" />
<!--            <stroke android:width="1dp"-->
<!--                android:color="#FFCC00"/>-->
        </shape>
    </item>
    <item android:gravity="center_horizontal" android:top="42dp" android:bottom="-12dp">
        <rotate android:fromDegrees="90" android:toDegrees="90">
            <shape
                android:shape="line">

                <stroke
                    android:width="1dp"
                    android:color="#DFC8C8"
                    android:dashWidth="1dp"
                    android:dashGap="2dp" />
            </shape>
        </rotate>
    </item>

    <item android:gravity="center">
        <shape android:shape="oval">

            <solid
                android:color="#FFCA99"/>

            <size
                android:width="8dp"
                android:height="8dp"/>
        </shape>
    </item>
</layer-list>