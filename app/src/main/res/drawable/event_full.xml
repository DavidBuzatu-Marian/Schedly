<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center">
        <shape android:shape="rectangle">
            <size
                android:width="48dp"
                android:height="48dp" />
<!--            <stroke-->
<!--                android:width="1dp"-->
<!--                android:color="#FFCC00" />-->
        </shape>
    </item>
    <item android:gravity="center">
        <shape android:shape="rectangle">


            <size
